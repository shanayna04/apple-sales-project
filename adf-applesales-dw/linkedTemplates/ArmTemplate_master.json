{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "adf-applesales-dw"
		},
		"ls_sa_apple_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"ls_sa_apple\""
		},
		"ls_sql_dw_apple_password": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"password\"de \"ls_sql_dw_apple\""
		},
		"ls_sql_dw_apple_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-ucb-sisinfo-applesales-123.database.windows.net"
		},
		"ls_sql_dw_apple_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dw_apple"
		},
		"ls_sql_dw_apple_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "valshany"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-applesales-dw_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sa_apple_connectionString": {
						"value": "[parameters('ls_sa_apple_connectionString')]"
					},
					"ls_sql_dw_apple_password": {
						"value": "[parameters('ls_sql_dw_apple_password')]"
					},
					"ls_sql_dw_apple_properties_typeProperties_server": {
						"value": "[parameters('ls_sql_dw_apple_properties_typeProperties_server')]"
					},
					"ls_sql_dw_apple_properties_typeProperties_database": {
						"value": "[parameters('ls_sql_dw_apple_properties_typeProperties_database')]"
					},
					"ls_sql_dw_apple_properties_typeProperties_userName": {
						"value": "[parameters('ls_sql_dw_apple_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}