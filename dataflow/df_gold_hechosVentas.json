{
	"name": "df_gold_hechosVentas",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_sales",
						"type": "DatasetReference"
					},
					"name": "sales"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_silver_warranty",
						"type": "DatasetReference"
					},
					"name": "warranty"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dimDate",
						"type": "DatasetReference"
					},
					"name": "dimDate"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dimDate",
						"type": "DatasetReference"
					},
					"name": "dimDate2"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dimProduct",
						"type": "DatasetReference"
					},
					"name": "dimProduct"
				},
				{
					"dataset": {
						"referenceName": "ds_gold_dimStore",
						"type": "DatasetReference"
					},
					"name": "dimStore"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_gold_hechosVentas",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "lookup1"
				},
				{
					"name": "lookup2"
				},
				{
					"name": "lookup3"
				},
				{
					"name": "lookup4"
				},
				{
					"name": "lookup5"
				},
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          sale_id as string,",
				"          sale_date as date,",
				"          store_id as string,",
				"          product_id as string,",
				"          quantity as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sales",
				"source(output(",
				"          claim_id as string,",
				"          claim_date as date,",
				"          sale_id as string,",
				"          repair_status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> warranty",
				"source(output(",
				"          id_date as integer,",
				"          date_full as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select id_date, date_full from gold.dim_date',",
				"     format: 'query') ~> dimDate",
				"source(output(",
				"          id_date as integer,",
				"          date_full as date",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select id_date, date_full from gold.dim_date',",
				"     format: 'query') ~> dimDate2",
				"source(output(",
				"          id_product as integer,",
				"          product as string,",
				"          price as decimal(10,2)",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select id_product, product, price from gold.dim_product',",
				"     format: 'query') ~> dimProduct",
				"source(output(",
				"          id_store as integer,",
				"          store as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select id_store, store from gold.dim_store',",
				"     format: 'query') ~> dimStore",
				"sales, warranty lookup(sales@sale_id == warranty@sale_id,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1, dimDate lookup(sale_date == date_full,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup2",
				"lookup2, dimDate2 lookup(claim_date == dimDate2@date_full,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup3",
				"lookup3, dimProduct lookup(product_id == product,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup4",
				"lookup4, dimStore lookup(store_id == store,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup5",
				"lookup5 select(mapColumn(",
				"          sale_id = sales@sale_id,",
				"          quantity,",
				"          repair_status,",
				"          id_date = dimDate@id_date,",
				"          id_claim_date = dimDate2@id_date,",
				"          id_product,",
				"          price,",
				"          id_store,",
				"          claim_date",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 derive(sale_amount = quantity+price,",
				"          claim_count = iif(isNull(claim_date),0,1)) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          sale_id,",
				"          quantity,",
				"          repair_status,",
				"          id_date,",
				"          id_claim_date,",
				"          id_product,",
				"          id_store,",
				"          sale_amount,",
				"          claim_count",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id_venta as integer,",
				"          id_date as integer,",
				"          id_claim_date as integer,",
				"          id_store as integer,",
				"          id_product as integer,",
				"          sale_id as string,",
				"          repair_status as string,",
				"          quantity_sold as integer,",
				"          sale_amount as decimal(10,2),",
				"          claim_count as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          id_date,",
				"          id_claim_date,",
				"          id_store,",
				"          id_product,",
				"          sale_id,",
				"          repair_status,",
				"          quantity_sold = quantity,",
				"          sale_amount,",
				"          claim_count",
				"     )) ~> sink1"
			]
		}
	}
}